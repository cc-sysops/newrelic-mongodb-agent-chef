newrelic:
  license_key: '<%= node['newrelic_mongodb_agent']['newrelic_license_key'] %>'
  #
  # Set to '1' for verbose output, remove for normal output.
  # All output goes to stdout/stderr.
  #
  #verbose: 1
#
# Agent Configuration:
#
agents:
  mongodb:
    agent_name: "<%= node['newrelic_mongodb_agent']['agent_name'] %>"
    endpoint: "<%= node['newrelic_mongodb_agent']['endpoint'] %>"
    database: "<%= node['newrelic_mongodb_agent']['database'] %>"
<%- if node['newrelic_mongodb_agent']['username'] %>
    username: "<%= node['newrelic_mongodb_agent']['username'] %>"
    password: "<%= node['newrelic_mongodb_agent']['password'] %>"
<% end %>